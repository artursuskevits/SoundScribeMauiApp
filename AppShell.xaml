<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SoundScribe.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SoundScribe.Views"
    Shell.FlyoutBehavior="Disabled">

    <Shell.Resources>
        <ResourceDictionary>
            <Style TargetType="Shell">
                <Setter Property="Shell.Title" Value="{StaticResource Primary}" />
                <Setter Property="Shell.TitleColor" Value="{StaticResource White}" />
            </Style>
        </ResourceDictionary>
    </Shell.Resources>

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:Flyuot2}"
        Route="Flyuot2" />

</Shell>
